user\"));\n      axios({\n        method: \"POST\",\n        url: \"/check_auth\",\n        data: {\n          id: _id,\n        },\n      })\n        .then((res) => {\n          dispatch({ type: \"LOGIN\", payload: res.data.user });\n          history.push(\"/mainscreen\");\n        })\n        .catch((err) => {\n          toast.error(<h2>{err.response.data.error}</h2>);\n          dispatch({ type: \"LOGOUT\" });\n          history.push(\"/\");\n        });\n    }\n  }, []);\n\n  return (\n    <>\n      <AuthContext.Provider value={{ state, dispatch }}>\n        <AnimatePresence exitBeforeEnter={true}>\n          <Switch location={location} key={location.key}>\n            <Route exact path=\"/\">\n              <Login />\n            </Route>\n\n            <Route exact path=\"/signup\">\n              <Signup />\n            </Route>\n            <Route exact path=\"/myprofile\">\n              <ProtectedRoute>\n                <MyProfile userinfo={state} />\n              </ProtectedRoute>\n            </Route>\n            <Route exact path=\"/mainscreen\">\n              <ProtectedRoute>\n                <Mainscreen setsearched={setsearched} />\n              </ProtectedRoute>\n            </Route>\n            <Route exact path=\"/search\">\n              <ProtectedRoute>\n                <Search searched={searched} />\n              </ProtectedRoute>\n            </Route>\n            <Route exact path=\"/user/:id\">\n              <ProtectedRoute>\n                <UserProfile />\n              </ProtectedRoute>\n            </Route>\n\n            <Route path=\"/*\" component={Error404} />\n          </Switch>\n        </AnimatePresence>\n\n        <ToastContainer autoClose={2500} />\n      </AuthContext.Provider>\n    </>\n  );\n}\n\nexport default App;\n","D:\\social media project\\ProfileNest\\client\\src\\reportWebVitals.js",[],"D:\\social media project\\ProfileNest\\client\\src\\components\\screens\\Signup.js",["132","133"],"D:\\social media project\\